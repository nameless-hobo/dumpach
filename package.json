{
  "name": "dumpach",
  "version": "1.0.0",
  "description": "SPA imageboard with one section without moderation",
  "main": "index.js",
  "engines": {
    "node": "6.9.4"
  },
  "scripts": {
    "start": "NODE_ENV=production node dist/server/index.js",
    "build": "webpack --config webpack.production.config.js && babel src/server -d dist/server",
    "build:front": "webpack --config webpack.production.config.js",
    "build:back": "babel src/server -d dist/server",
    "dev": "nodemon src/server/index.js --watch src/server --ignore src/client --ignore uploads/ --ignore uploads_thumbs/ --exec babel-node NODE_ENV=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nameless-hobo/dumpach.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nameless-hobo/dumpach/issues"
  },
  "homepage": "https://github.com/nameless-hobo/dumpach#readme",
  "devDependencies": {
    "axios": "^0.15.3",
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.1.2",

    "node-sass": "^3.13.1",
    "nodemon": "^1.11.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "sass-loader": "^4.0.2",
    "semantic-ui-react": "^0.68.5",
    "style-loader": "^0.13.1",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "react-router": "^3.0.1",
    "react-router-redux": "^4.0.8"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "body-parser": "^1.16.0",
    "express": "^4.14.0",
    "fluent-ffmpeg": "^2.1.2",
    "formidable": "^1.0.17",
    "morgan": "^1.8.2",
    "sharp": "^0.17.3"
  }
}
